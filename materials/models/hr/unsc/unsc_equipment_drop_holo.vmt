"UnlitGeneric"
{
// File Created by Wall Worm Pro ( http://dev.wallworm.com )

// BEGIN Textures
	$basetexture "models/hr/unsc/unsc_equipment_drop_holo2"
	$detail "models/hr/unsc/unsc_equipment_drop_holo_pixels"
	$additive 1
	$detailblendfactor 1
	$detailblendmode 1
// END Textures

	$cvar "[.5 .5]"
	$svar "[1 1]"
	$rvar "[0]"
	$tvar "[0 0]"
	$ramp "0.0"
	
	"Proxies"
	{
		LinearRamp
		{
			rate 0.02
			initialValue 0
			resultVar $ramp
		}

		WrapMinMax
		{
			srcVar1 "$ramp"
			minVal -0.01
			maxVal 0.01
			resultVar "$tvar[1]"
		}
		TextureTransform
		{
			centervar $cvar
			scalevar $svar
			rotatevar $rvar
			translatevar $tvar
			resultvar $basetexturetransform
		}


		TextureScroll
		{
			textureScrollVar $detailtexturetransform
			textureScrollRate 0.035
			textureScrollAngle 180
		}

	}

}